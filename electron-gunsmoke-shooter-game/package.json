{
    "name": "electron-gunsmoke-shooter-game",
    "version": "1.0.0",
    "description": "Gunsmoke is an electron application for a shooter game implemented in pure javascript.",
    "main": "src/app.js",
    "scripts": {
        "start": "electron .",
        "dist": "electron-builder",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
        "package-win": "electron-packager . electron-gunsmoke-shooter-game --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"electron-gunsmoke-shooter-game\"",
        "package-linux": "electron-packager . electron-gunsmoke-shooter-game --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds"
    },
    "devDependencies": {
        "electron": "~1.8.3",
        "electron-builder": "^20.3.1",
        "electron-packager": "^11.1.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/andreros/playground/tree/master/electron-gunsmoke-shooter-game"
    },
    "homepage": "https://github.com/andreros/playground/tree/master/electron-gunsmoke-shooter-game#readme",
    "keywords": [
        "electron",
        "gunsmoke",
        "shooter",
        "game"
    ],
    "author": "Raj Kumar <https://www.facebook.com/hackmanraj>",
    "contributors": [
        "Andr√© Rosa <andreros@gmail.com>"
    ],
    "license": "Unlicense"
}
