{{#toDoListApp}}
<div class="toDoListApp">
    <header>
        <nav>
            <div class="nav-wrapper">
                <a href="javascript:void();" class="brand-logo">&nbsp;&nbsp;{{name}}</a>
            </div>
        </nav>
    </header>

    <main>
        <br/>
        <div class="row">
            <div class="col s12">
                <h5>{{taskListTitle}}</h5>
            </div>
        </div>

        <div class="row">
            <div class="col s12">
                <table class="striped highlight">
                    <thead>
                        <tr>
                            <th class="actions">&nbsp;</th>
                            <th class="numbers">#</th>
                            <th>Task Name</th>
                        </tr>
                    </thead>

                    <tbody id="taskList">
                        <tr>
                            <td>
                                <button class="btn-floating btn-delete-task waves-effect waves-light black" type="button">
                                    <i class="material-icons">delete</i>
                                </button>
                            </td>
                            <td class="numbers">1</td>
                            <td>Task 1</td>
                        </tr>
                        <tr>
                            <td>
                                <button class="btn-floating btn-delete-task waves-effect waves-light black" type="button">
                                    <i class="material-icons">delete</i>
                                </button>
                            </td>
                            <td class="numbers">2</td>
                            <td>Task 2</td>
                        </tr>
                        <tr>
                            <td>
                                <button class="btn-floating btn-delete-task waves-effect waves-light black" type="button">
                                    <i class="material-icons">delete</i>
                                </button>
                            </td>
                            <td class="numbers">3</td>
                            <td>Task 3</td>
                        </tr>
                        <tr>
                            <td>
                                <button class="btn-floating btn-delete-task waves-effect waves-light black" type="button">
                                    <i class="material-icons">delete</i>
                                </button>
                            </td>
                            <td class="numbers">4</td>
                            <td>Task 4</td>
                        </tr>
                        <tr>
                            <td>
                                <button class="btn-floating btn-delete-task waves-effect waves-light black" type="button">
                                    <i class="material-icons">delete</i>
                                </button>
                            </td>
                            <td class="numbers">5</td>
                            <td>Task 5</td>
                        </tr>
                        <tr>
                            <td>
                                <button class="btn-floating btn-delete-task waves-effect waves-light black" type="button">
                                    <i class="material-icons">delete</i>
                                </button>
                            </td>
                            <td class="numbers">6</td>
                            <td>Task 6</td>
                        </tr>
