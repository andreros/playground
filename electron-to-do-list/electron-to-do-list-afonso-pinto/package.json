{
  "name": "electrontodolist",
  "version": "1.0.0",
  "description": "To-Do List desktop application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . electronToDoList --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"ElectronToDoList\"",
    "package-linux": "electron-packager . electronToDoList --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/afonsobspinto/ElectronToDoList.git"
  },
  "author": "Afonso Pinto",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/afonsobspinto/ElectronToDoList/issues"
  },
  "homepage": "https://github.com/afonsobspinto/ElectronToDoList#readme",
  "dependencies": {
    "electron": "^1.7.9",
    "typescript-boilerplate": "https://github.com/andreros/typescript-boilerplate.git"
  },
  "devDependencies": {
    "electron-packager": "^9.1.0"
  }
}
