{
    "name": "electron-to-do-list",
    "version": "1.0.0",
    "description": "To Do List application built with Electron, Typescript, SASS / Materialize for CSS and Handlebars for HTML templating.",
    "main": "dist/index.js",
    "scripts": {
        "start": "gulp",
        "dev": "gulp default",
        "serve": "gulp serve",
        "electron": "electron .",
        "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=dist/assets/icons/mac/icon.icns --prune=true --out=release-builds",
        "package-win": "electron-packager . electron-to-do-list --overwrite --asar=true --platform=win32 --arch=ia32 --icon=dist/assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"electron-to-do-list\"",
        "package-linux": "electron-packager . electron-to-do-list --overwrite --asar=true --platform=linux --arch=x64 --icon=dist/assets/icons/png/icon.png --prune=true --out=release-builds"
    },
    "dependencies": {
        "electron": "^1.8.2",
        "hammerjs": "^2.0.8",
        "jquery": "^3.3.1"
    },
    "devDependencies": {
        "@types/handlebars": "^4.0.36",
        "browser-sync": "^2.23.6",
        "electron-packager": "^11.1.0",
        "gulp": "^3.9.1",
        "gulp-autoprefixer": "^4.1.0",
        "gulp-concat": "^2.6.1",
        "gulp-hb": "^7.0.1",
        "gulp-merge-json": "^1.2.1",
        "gulp-rename": "^1.2.2",
        "gulp-sass": "^3.1.0",
        "gulp-sourcemaps": "^2.6.4",
        "gulp-tslint": "^8.1.3",
        "gulp-typescript": "^4.0.1",
        "handlebars": "^4.0.11",
        "normalize-scss": "^7.0.1",
        "rimraf": "^2.6.2",
        "tslint": "^5.9.1",
        "typescript": "^2.7.1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/andreros/playground/tree/master/electron-to-do-list"
    },
    "homepage": "https://github.com/andreros/playground/tree/master/electron-to-do-list#readme",
    "keywords": [
        "to do list",
        "electron",
        "typescript",
        "SASS",
        "handlebars"
    ],
    "author": "Andr√© Rosa <andreros@gmail.com>",
    "contributors": [],
    "license": "Unlicense"
}
